{
  "name": "libraryapi",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start-webapp:dev": "cd webapp && npm run serve:dev",
    "start-webapp:preprod": "cd webapp && npm run serve:preprod",
    "start-webapp:prod": "cd webapp && npm run serve:prod",
    "start-api:dev": "cd webapi && npm run start:debug",
    "start-api:preprod": "cd webapi && npm run serve",
    "start:dev": "concurrently \"npm run start-api:dev\" \"npm run start-webapp:dev\"",
    "start:preprod": "concurrently \"npm run start-api:preprod\" \"npm run start-webapp:preprod\"",
    "start:prod": "npm run start-webapp:prod",
    "deploy:webapi": "cd webapi && npm run deploy",
    "deploy:webapp": "cd webapp && npm run deploy",
    "deploy:all": "concurrently \"npm run deploy:webapi\" \"npm run deploy:webapp\""
  },
  "private": true,
  "dependencies": {
    "concurrently": "^9.1.2"
  }
}
